/* pages/list.wxss */
.count-list{
  padding: 30rpx 0;
  border-bottom: 1px solid #eee;
  display: flex;
  flex-direction: row;
}
.count-list-left{
  flex: 1;
  display: flex;
  flex-direction: column;
}
.count-list-right{
  display: flex;
  flex-direction: column;
  align-items:flex-end;
}
.count-list-price{
  color: #f00;
  font-size: 32rpx;
}
.count-list-tip{
  color: #999;
  font-size: 32rpx;
  margin-top: 8rpx;
}